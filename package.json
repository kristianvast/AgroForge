{
  "name": "agroforge-workspace",
  "version": "0.9.1",
  "private": true,
  "description": "AgroForge monorepo workspace",
  "workspaces": {
    "packages": [
      "packages/server",
      "packages/ui",
      "packages/electron-app",
      "packages/tauri-app",
      "packages/cloudflare",
      "packages/opencode-config"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "npm run dev --workspace @agroforge/electron-app",
    "dev:electron": "npm run dev --workspace @agroforge/electron-app",
    "dev:tauri": "npm run dev --workspace @agroforge/tauri-app",
    "build": "npm run build --workspace @agroforge/electron-app",
    "build:tauri": "npm run build --workspace @agroforge/tauri-app",
    "build:ui": "npm run build --workspace @agroforge/ui",
    "build:mac-x64": "npm run build:mac-x64 --workspace @agroforge/electron-app",
    "build:binaries": "npm run build:binaries --workspace @agroforge/electron-app",
    "typecheck": "npm run typecheck --workspace @agroforge/ui && npm run typecheck --workspace @agroforge/electron-app",
    "bumpVersion": "npm version --workspaces --include-workspace-root --no-git-tag-version"
  },
  "dependencies": {
    "7zip-bin": "^5.2.0",
    "fdir": "^6.5.0",
    "google-auth-library": "^10.5.0",
    "lru-cache": "^11.2.5",
    "picomatch": "^4.0.3",
    "yallist": "^5.0.0"
  },
  "devDependencies": {
    "baseline-browser-mapping": "^2.9.11"
  },
  "overrides": {
    "tar": "^7.5.4"
  },
  "license": "MIT",
  "author": {
    "name": "Kristian Vastveit",
    "url": "https://github.com/kristianvast"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kristianvast/AgroForge.git"
  },
  "homepage": "https://github.com/kristianvast/AgroForge",
  "bugs": {
    "url": "https://github.com/kristianvast/AgroForge/issues"
  }
}
