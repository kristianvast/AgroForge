{
  "name": "@agroforge/server",
  "version": "0.9.1",
  "description": "AgroForge Server",
  "author": {
    "name": "Kristian Vastveit",
    "url": "https://github.com/kristianvast"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kristianvast/AgroForge.git"
  },
  "homepage": "https://github.com/kristianvast/AgroForge",
  "bugs": {
    "url": "https://github.com/kristianvast/AgroForge/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agroforge": "dist/bin.js"
  },
  "scripts": {
    "build": "npm run build:ui && npm run prepare-ui && tsc -p tsconfig.json && node ./scripts/copy-auth-pages.mjs && npm run prepare-config",
    "build:ui": "npm run build --prefix ../ui",
    "prepare-ui": "node ./scripts/copy-ui-dist.mjs",
    "prepare-config": "node ./scripts/copy-opencode-config.mjs",
    "dev": "cross-env CODENOMAD_DEV=1 CODENOMAD_SERVER_PASSWORD=codenomad-dev CLI_UI_DEV_SERVER=http://localhost:3000 tsx src/index.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/reply-from": "^9.8.0",
    "@fastify/static": "^6.12.0",
    "commander": "^12.1.0",
    "fastify": "^4.28.1",
    "fuzzysort": "^2.0.4",
    "pino": "^9.4.0",
    "undici": "^6.19.8",
    "yauzl": "^2.10.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/yauzl": "^2.10.0",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  }
}
